<%= form_for ([@commenter, @comment]) do |f| %>
  <p>
    <%= f.label :commenter %><br>
    <%= f.text_field :commenter %>
  </p>

  <p>
    <%= f.label :body %><br>
    <%= f.text_area :body %>
  </p>

  <p>
    <%= f.submit %>
    <p>Comment type is: <%= @comment.commentable_type %></p>
  </p>

<% end %>
